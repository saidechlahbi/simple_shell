main.c
*main - simple shelle main function
*@ac : count of argument
*@av : argument
*Return : 0 always (succes)
*/
int main (int ac, char **argv)
{
   char *line =NULL;
   char **command =NULL;
   int status ;
   (void)ac;
   while(1)
   {
   line =read_line ();
      if (line == NULL)/*reach EOF ctr + D*/
      {
         if (issaty (STDIN_FILENO))
         write(STDOUT_FILEENO, "\n",1);
         return (status);
      }
   commande = toknizer (line);
      if (!=command)
      continue;
   status = execute(commande.argv);
